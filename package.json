{
    "name": "@ambak/nest-logger",
    "version": "1.0.13",
    "description": "Structured logging for NestJS applications",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist/**/*",
        "README.md"
    ],
    "scripts": {
        "build": "rimraf dist && tsc",
        "build:watch": "rimraf dist && tsc --watch",
        "dev": "npm run build:watch",
        "prepare": "npm run build",
        "test": "jest",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
        "format": "prettier --write \"src/**/*.ts\"",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "nestjs",
        "logging",
        "pino",
        "structured-logging"
    ],
    "author": "Ambak",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ambak-fintech/ambak-nest-logger.git"
    },
    "peerDependencies": {
        "@nestjs/common": "^10.0.0 || ^11.0.0",
        "@nestjs/core": "^10.0.0 || ^11.0.0",
        "reflect-metadata": "^0.1.13 || ^0.2.0",
        "rxjs": "^7.8.1"
    },
    "optionalDependencies": {
        "@nestjs/graphql": "^12.0.0 || ^13.0.0"
    },
    "dependencies": {
        "pino": "^8.14.1",
        "pino-pretty": "^10.0.0",
        "safe-stable-stringify": "^2.5.0"
    },
    "devDependencies": {
        "@nestjs/common": "^10.0.0 || ^11.0.0",
        "@nestjs/core": "^10.0.0 || ^11.0.0",
        "@nestjs/graphql": "^12.0.0 || ^13.0.0",
        "@nestjs/platform-express": "^10.0.0 || ^11.0.0",
        "@nestjs/testing": "^10.0.0 || ^11.0.0",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "^20.3.1",
        "@typescript-eslint/eslint-plugin": "^5.59.11",
        "@typescript-eslint/parser": "^5.59.11",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "typescript": "^5.1.3"
    }
}
